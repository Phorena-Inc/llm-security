{
  "test_suite_name": "AI Privacy Firewall - Team C Test Cases",
  "version": "1.0",
  "description": "Comprehensive test cases for privacy ontology and decision making",
  "8_week_demo_scenarios": [
    {
      "test_id": "DEMO_001",
      "name": "Emergency Access",
      "description": "8-week project demo: Emergency medical access",
      "input": {
        "requester": "dr_emergency",
        "data_field": "patient_medical_record", 
        "purpose": "emergency_treatment",
        "context": "medical",
        "emergency": true
      },
      "expected_result": {
        "allowed": true,
        "should_contain_reason": ["emergency override"],
        "min_confidence": 0.95
      }
    },
    {
      "test_id": "DEMO_002",
      "name": "Temporal Boundaries", 
      "description": "8-week project demo: Contractor financial access denial",
      "input": {
        "requester": "contractor_john",
        "data_field": "project_financial_data",
        "purpose": "project_work",
        "context": "financial",
        "emergency": false
      },
      "expected_result": {
        "allowed": false,
        "should_contain_reason": ["financial data requires", "HR authorization"],
        "min_confidence": 0.85
      }
    },
    {
      "test_id": "DEMO_003",
      "name": "Organizational Hierarchy",
      "description": "8-week project demo: Manager team data access",
