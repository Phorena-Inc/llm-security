        "data_field": "team_performance_data",
        "purpose": "management_review",
        "context": "hr", 
        "emergency": false
      },
      "expected_result": {
        "allowed": true,
        "should_contain_reason": ["standard access policy"],
        "min_confidence": 0.75
      }
    },
    {
      "test_id": "DEMO_004",
